		"@tailwindcss/typography": "^0.5.19",
		"@eslint/compat": "^1.4.0",
		"@eslint/js": "^9.39.1",
		"@sveltejs/adapter-netlify": "^5.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^6.2.4",
		"@types/node": "^22",
		"autoprefixer": "^10.4.24",
		"daisyui": "^5.5.14",
		"eslint": "^9.39.1",
