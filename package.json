		"svelte": "^5.45.6",
		"svelte-check": "^4.3.4",
		"tailwindcss": "^3.4.19",
		"tsx": "^4.19.2",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.48.1",
		"vite": "^7.2.6"
