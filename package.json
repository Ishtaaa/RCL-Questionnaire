		"lint": "prettier --check . && eslint .",
		"migrate": "tsx scripts/migrate-json-to-db.ts",
		"db:init": "tsx scripts/init-db.ts",
		"db:export": "tsx scripts/export-responses-to-json.ts"
	},
	"dependencies": {
		"@netlify/neon": "^0.1.0",
